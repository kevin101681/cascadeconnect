{
  "name": "cascade-connect",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "server": "node server/index.js",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "create-admin": "tsx scripts/create-admin.ts",
    "push": "powershell -ExecutionPolicy Bypass -File ./scripts/auto-push.ps1",
    "push:msg": "powershell -ExecutionPolicy Bypass -File ./scripts/auto-push.ps1"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.0.0",
    "@clerk/clerk-sdk-node": "^5.0.0",
    "@google/genai": "^1.33.0",
    "@neondatabase/serverless": "^0.10.1",
    "@uploadthing/react": "^6.3.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.1",
    "express": "^4.18.2",
    "jspdf": "^2.5.1",
    "lucide-react": "^0.344.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "uploadthing": "^6.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.30.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
