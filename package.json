{
  "name": "cascade-connect",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "dev:client": "vite",
    "server": "node server/index.js",
    "build": "tsc && vite build && npm run copy-html && npm run copy-manual-images && npm run verify-build",
    "verify-build": "node -e \"const fs = require('fs'); const path = require('path'); console.log('\\n=== Build Verification ==='); const files = ['complete_homeowner_manual.html', 'homeowner-manual.html', '_redirects', '_headers']; files.forEach(f => { const exists = fs.existsSync(path.join('dist', f)); console.log(exists ? '✓' : '✗', f, exists ? 'exists' : 'MISSING'); }); console.log('=========================\\n');\"",
    "copy-html": "node -e \"const fs = require('fs'); const path = require('path'); fs.mkdirSync(path.join('dist', 'static'), {recursive: true}); ['complete_homeowner_manual.html', 'homeowner-manual.html'].forEach(f => { const src = path.join('public', f); const dest = path.join('dist', f); try { fs.copyFileSync(src, dest); console.log('✓ Copied', f, 'to dist'); } catch(e) { console.error('✗ Failed to copy', f, e.message); } }); try { fs.copyFileSync(path.join('public', 'static', 'manual.html'), path.join('dist', 'static', 'manual.html')); console.log('✓ Copied manual.html to dist/static'); } catch(e) { console.error('✗ Failed to copy manual.html', e.message); }\"",
    "copy-manual-images": "node -e \"const fs = require('fs'); const path = require('path'); const srcDir = path.join('public', 'images', 'manual'); const destDir = path.join('dist', 'images', 'manual'); fs.mkdirSync(destDir, {recursive: true}); fs.readdirSync(srcDir).forEach(f => { fs.copyFileSync(path.join(srcDir, f), path.join(destDir, f)); console.log('✓ Copied image', f); });\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "db:push": "drizzle-kit push",
    "db:push:script": "powershell -ExecutionPolicy Bypass -File ./scripts/push-schema.ps1",
    "db:studio": "drizzle-kit studio",
    "create-admin": "tsx scripts/create-admin.ts",
    "check-admin-users": "tsx scripts/check-admin-users.ts",
    "add-email-preferences": "tsx scripts/add-email-preferences.ts",
    "create-contractors-table": "tsx scripts/create-contractors-table.ts",
    "create-message-threads-table": "tsx scripts/create-message-threads-table.ts",
    "create-missing-tables": "tsx scripts/create-missing-tables.ts",
    "create-calls-table": "tsx scripts/create-calls-table.ts",
    "verify-calls-table": "tsx scripts/verify-calls-table.ts",
    "create-sms-messages-table": "tsx scripts/create-sms-messages-table.ts",
    "create-sms-messages-indexes": "tsx scripts/create-sms-messages-indexes.ts",
    "add-tasks-fields": "tsx scripts/add-tasks-fields.ts",
    "create-email-logs-table": "tsx scripts/create-email-logs-table.ts",
    "create-appointments-tables": "tsx scripts/create-appointments-tables.ts",
    "create-internal-chat-tables": "tsx scripts/create-internal-chat-tables.ts",
    "create-push-notifications-tables": "tsx scripts/create-push-notifications-tables.ts",
    "setup-push-notifications": "tsx scripts/create-push-notifications-tables.ts",
    "disable-user-mfa": "tsx scripts/disable-user-mfa.ts",
    "check-cloudinary": "node scripts/check-cloudinary.js",
    "migrate:cbsbooks": "tsx scripts/migrate-cbsbooks.ts",
    "create-cbsbooks-tables": "tsx scripts/create-cbsbooks-tables.ts",
    "diagnose:cbsbooks": "tsx scripts/diagnose-cbsbooks.ts",
    "add-updated-at-column": "tsx scripts/add-updated-at-column.ts",
    "push": "powershell -ExecutionPolicy Bypass -File ./scripts/auto-push.ps1",
    "push:msg": "powershell -ExecutionPolicy Bypass -File ./scripts/auto-push.ps1",
    "netlify:login": "netlify login",
    "netlify:init": "netlify init",
    "netlify:deploy": "netlify deploy",
    "netlify:deploy:prod": "netlify deploy --prod",
    "netlify:status": "netlify status",
    "verify:package": "node scripts/verify-package-json.js"
  },
